<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.own.hcanim">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.own.hcanim.BaseAnimActivity" />
        <activity android:name="com.own.hcanim.AnimListenActivity" />
        <activity android:name="com.own.hcanim.CombinationAnimActivity"></activity>
        <activity android:name="com.own.hcanim.RotateAnimActivity"></activity>
        <activity android:name="com.own.hcanim.MediaTestActivity"></activity>

    </application>

</manifest>